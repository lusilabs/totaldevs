{
  "name": "totaldevs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "all": "next dev & yarn emulators & yarn stripe",
    "_emulators": "lsof -ti:5000,5001,9099,8080,9000,8085,9199,9200 | xargs kill && firebase emulators:start",
    "emulators": "yarn _emulators --import=./.firebase/emulators --export-on-exit",
    "stripe": "stripe listen --forward-to http://127.0.0.1:5001/totaldevs-25387/us-central1/stripe-handleWebhooks --forward-connect-to http://127.0.0.1:5001/totaldevs-25387/us-central1/stripe-handleWebhooks",
    "build": "next build && next export",
    "debug": "NODE_OPTIONS='--inspect' next dev & firebase emulators:start '--inspect-functions'",
    "deploy-preview": "yarn build && firebase hosting:channel:deploy mvp",
    "deploy-production": "yarn build && firebase deploy",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && yarn install && yarn dev"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@stripe/stripe-js": "^1.19.0",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "faker": "^5.5.3",
    "firebase": "^9.0.2",
    "next": "11.1.2",
    "next-pwa": "^5.4.0",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebase-hooks": "^3.0.4",
    "react-hook-form": "^7.17.2",
    "react-toastify": "^8.0.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.4",
    "tailwindcss": "^2.2.19"
  }
}
